pipeline {
    agent any

    stages {
        stage('Pull ImÃ¡genes Docker') {
            steps {
                script {
                    def images = [
                        'ubuntu',
                        'nginx',
                        'mongo',
                        'redis',
                        'python'
                    ]
                    images.each { image ->
                        sh "docker pull ${image}:latest"
                    }
                }
            }
        }
    }
}
